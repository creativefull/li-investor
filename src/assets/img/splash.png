<script type="text/javascript">

var fieldErrors={};
var actionErrors=[];
  

$(".button.submit-button").click(function() {
   $(this).addClass("loading");
   setTimeout(
  	function() {
  	$(".errorTop").hide();
    $(".errorInput").removeClass('errorInput');
    
    var data = {
        url: $("#usernote-login input[name='url']").val(),
        username: $("#usernote-login #email").val(),
        password: $("#usernote-login #password").val(),
        rememberMe: $("#rememberMe").val(),
        "struts.token.name": "token",
        token: $("#usernote-login input[name='token']").val(),
        "popup": false,
        "onlyOwners": false,
        "onlyAdmins": false
        };
        
    var extraData = {};
    if(typeof analyticsModel !== 'undefined' && analyticsModel !== null){
      extraData = {
                    clientId:           analyticsModel.get('clientId'),
                    dataSource:         analyticsModel.get('dataSource'),
                    referrer:           analyticsModel.get('referrer'),
                    campaignName:       analyticsModel.get('campaignName'),
                    campaignSource:     analyticsModel.get('campaignSource'),
                    campaignMedium:     analyticsModel.get('campaignMedium'),
                    campaignKeyword:    analyticsModel.get('campaignKeyword'),
                    campaignContent:    analyticsModel.get('campaignContent'),
                    campaignId:         analyticsModel.get('campaignId'),
                    screenResolution:   analyticsModel.get('screenResolution'),
                    viewportSize:       analyticsModel.get('viewportSize'),
                    encoding:           analyticsModel.get('encoding'),
                    screenColors:       analyticsModel.get('screenColors'),
                    language:           analyticsModel.get('language')
                };
      data = $.extend(data, extraData);
    }
    
    $(".button.submit-button").ajaxSubmit({
        type: "POST",


        url: '/usernote/doLogin.action',

        dataType: "json",
        data: data,
        success: function(json, status, xhr) {
        	$(".overlaySignIn").css("display", "none");
			
        	if(json && json.result && json.result==="success") {
				if(window.parent){
					window.parent.location.href=json.redirectTo + window.location.hash;
				} else {
					document.location.replace(json.redirectTo);	
				}	
			} else if(json && json.result && json.result==="multiple-account-access") {
				 	$("#usernote-login > div").last().fadeOut(500, function() {
    				$("#usernote-login > div").last().html($.parseJSON(xhr.responseText).content);	
    				$("#usernote-login > div").last().fadeIn(500, function() {
    					if($('#listUsers').scrollTop() == 0)
    						$('#listUsers').animate({
                    			scrollTop: $("#listUsers input:checked").parent().offset().top - $("#listUsers").offset().top - $("#listUsers input:checked").parent().outerHeight(true) - 10 
                			}, 500);
    				});
  				});
			} else {
				showErrors(json.fieldErrors, json.actionErrors);
				var tokenName=$("#usernote-login input[name='struts.token.name']").val();
				$("input[name='"+ tokenName +"']").val(json.t);
			}
        },
    	error: function (xhr, status, error) {
    		$(".overlaySignIn").css("display", "none");
    		$(".submit-button").removeClass("loading");
			$(".errorTop").html("Unknown error!!").show('fast');
    	}
    });
  }, 1000);
});

</script>		
	

